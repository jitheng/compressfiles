{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/compress.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/blob-upload.js": {
      "maxDuration": 10,
      "memory": 256
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
